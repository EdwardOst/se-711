commonLoging/commonLoging=org.apache.commons.logging.Log\=org.apache.commons.logging.impl.Log4JLogger
eclipse.preferences.version=1
isNewProject/isNewProject=false
log4jContent/log4jContent=<?xml version\="1.0" encoding\="UTF-8"?>\r\n<Configuration>\r\n    <Properties>\r\n        <Property name\="logPath">C\:/tmp/talend/logs</Property>\r\n        <Property name\="rollingFileName">studio</Property>\r\n    </Properties>\r\n    <Appenders>  \r\n        <Console name\="Console" target\="SYSTEM_OUT">  \r\n            <PatternLayout pattern\="[%-5level] %d{HH\:mm\:ss} %x %X %logger{36}- %msg%n%throwable{short.lineNumber}" />\r\n        </Console>\r\n\r\n        <RollingFile name\="TalendRollingLog" fileName\="${logPath}/${rollingFileName}.log" filePattern\="${logPath}/${rollingFileName}_%d{yyyy-MM-dd-HH}-%i.log">\r\n            <PatternLayout>\r\n                <Pattern>[%-5level] %d{HH\:mm\:ss} %x %X %logger{36}- %msg%n%throwable{short.lineNumber}</Pattern>\r\n           </PatternLayout>\r\n           <Policies>\r\n               <\!-- Causes a rollover if the log file is older than the current JVM's start time -->\r\n               <OnStartupTriggeringPolicy />\r\n               <\!-- Causes a rollover once the date/time pattern no longer applies to the active file -->\r\n               <TimeBasedTriggeringPolicy interval\="1" modulate\="true" />\r\n           </Policies>\r\n        </RollingFile>\r\n        <Async name\="TalendRollingLog_Async">\r\n            <AppenderRef ref\="TalendRollingLog"/>\r\n        </Async>\r\n    </Appenders>  \r\n    \r\n    <Loggers>\r\n        <Logger name\="se_711" level\="INFO" additivity\="true">\r\n            <AppenderRef ref\="TalendRollingLog_Async" />  \r\n\t\t</Logger>\r\n        <Root level\="DEBUG">\r\n            <AppenderRef ref\="Console" />  \r\n        </Root>  \r\n    </Loggers>  \r\n\r\n</Configuration>
log4jEnabled/log4jEnabled=true
log4jVersion2/log4jVersion2=true
